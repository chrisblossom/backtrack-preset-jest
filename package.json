{
  "name": "@backtrack/preset-jest",
  "version": "0.1.10",
  "description": "backtrack preset for testing node with Jest",
  "keywords": [
    "backtrack",
    "preset",
    "testing",
    "jest",
    "wallaby"
  ],
  "license": "ISC",
  "repository": "chrisblossom/backtrack-preset-jest",
  "homepage": "https://github.com/chrisblossom/backtrack-preset-jest",
  "author": "Chris Blossom",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.6.0"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/preset-jest.js",
  "scripts": {
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "backtrack precommit",
    "prepublishOnly": "backtrack prepublishOnly",
    "prepush": "backtrack prepush",
    "test": "backtrack test",
    "test.ci": "backtrack test.ci",
    "test.update": "backtrack test.update",
    "test.watch": "backtrack test.watch"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "backtrack": {
    "presets": [
      "@backtrack/preset"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.13",
    "@backtrack/preset-preset": "^0.1.8"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-serializer-path": "^0.1.15",
    "jest-snapshot-serializer-function-name": "^1.1.0"
  }
}
